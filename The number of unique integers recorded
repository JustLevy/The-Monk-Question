# The Monk Question
def integer_division_results(n):
    results_set = set()
    
    for i in range(1, n+1):
        result = n // i
        results_set.add(result)
    
    return len(results_set)

# Take user input
n = int(input("Enter a number n: "))

# Get the results and count
count = integer_division_results(n)

# Print the results
print(f"Number of unique elements in the set: {count}")
